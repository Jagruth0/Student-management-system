<%
    function letterGrade(grade) {
        switch (grade) {
            case 10:
                return "A1"
                break;
            case 9:
                return "A2"
                break;
            case 8:
                return "B1"
                break;
            case 7:
                return "B2"
                break;
            case 6:
                return "C1"
                break;
            case 5:
                return "C2"
                break;
            case 4:
                return "D"
                break;
            default:
                return "F"
                break;
        }
    }

%>